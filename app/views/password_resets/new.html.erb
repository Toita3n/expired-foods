<div class="content">
 <div class="row">
   <p>パスワード変更申請</p>
   <%= form_with url: password_resets_path, local: true, method: :post do |form| %>
     <div class="field">
       <%= form.label :email %><br />
       <%= form.text_field :email %>
       <%= form.submit "送信", class:"btn btn-primary" %>
     </div>
   <% end %>
  </div>
</div>