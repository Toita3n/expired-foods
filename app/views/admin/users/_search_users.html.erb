<tr>
  <td>
    <%= search_user.id %>
  </td>
  <td>
    <%= search_user.email %>
  </td>
  <td>
    <%= search_user.role_i18n %>
  </td>
  <td>
    <%= l search_user.created_at, format: :long %>
  </td>
  <td>
    <%= link_to t('defaults.show'), admin_user_path(user), class: 'btn btn-info' %>
    <%= link_to t('defaults.edit'), edit_admin_user_path(user), class: 'btn btn-success' %>
    <%= link_to t('defaults.delete'), admin_user_path(user), method: :delete, data: { confirm: t('defaults.message.deleted_comfirm') }, class: 'btn btn-danger' %>
  </td>
</tr>